{
  "name": "nextjs-14-course-react-refresher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": "^20.10.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "yarn eslint && yarn stylelint && yarn prettier",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix && yarn prettier:fix",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "prettier": "prettier --config .prettierrc.cjs 'src/**/*.{js,ts,tsx}' --check",
    "prettier:fix": "prettier --config .prettierrc.cjs 'src/**/*.{js,ts,tsx}' --write",
    "preview": "vite preview",
    "postinstall": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "classnames": "^2.3.2",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.0",
    "sass": "^1.68.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-svgr": "^4.2.0"
  }
}
